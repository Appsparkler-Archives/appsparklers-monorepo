{
  "name": "appsparklers",
  "private": true,
  "scripts": {
    "_mhw:storybook": "turbo storybook --filter=@repo/pages-mhw",
    "_mhw:css:watch": "turbo css:watch --filter=@repo/pages-mhw",
    "mhw:storybook": "run-p _mhw:css:watch _mhw:storybook"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "pages/*",
    "apps/*"
  ],
  "devDependencies": {
    "npm-run-all2": "^7.0.2",
    "turbo": "^2.3.3"
  }
}
