{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "storybook": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "inputs": [
        "lib/**/*.{ts,tsx}",
        "tsconfig.build.json",
        "tsconfig.app.json"
      ],
      "outputs": ["dist"]
    }
  }
}
